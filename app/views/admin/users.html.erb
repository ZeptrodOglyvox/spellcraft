<h2>Admin Users</h2>
<table>
    <tr>
        <th width="30%">Username</th>
        <th width="20%">Signed Up</th>
        <th width="20%">Last Signed In On</th>
    </tr>
    <% @users.each do |user| %>
    <tr>
        <td><%= user.username %></td>
        <td><%= user.signed_up_on %></td>
        <td><%= user.last_sign_in %></td>
        <td><%= link_to 'Delete', user, confirm: 'Are you sure?', method: :delete %></td>
    </tr>
    <% end %>
</table>